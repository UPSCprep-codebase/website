          <defs>
            <clipPath id="clip0">
              <path transform="rotate(148.25 56.2 41.845)" fill="#fff" d="M56.2 41.845h35.638v49.209H56.2z"></path>
            </clipPath>
          </defs>
        </svg>
      </div>
    </div>
    <div class="home__topics">

      <a class="home__topic-block tc color-text nolink mt2" href="/upsc-mains-daily-answer-writing/">
        <div class="fs45 mb1">üìù</div>
        <h2 class="mb1">Daily Answer Writing</h2>
        <p>We post 2 questions with model solutions every Mon to Fri. Practice makes perfect.</p>
      </a>

      <a class="home__topic-block tc color-text nolink mt2" href="/csat/">
        <div class="fs45 mb1">üî†</div>
        <h2 class="mb1">CSAT Free Resources</h2>
        <p>CSAT theory, practice questions, tips and tricks.</p>
      </a>

      <a class="home__topic-block tc color-text nolink mt2" href="/sociology-notes/">
        <div class="fs45 mb1">üìö</div>
        <h2 class="mb1">Sociology Optional Notes</h2>
        <p>Detailed notes for Sociology optional paper 1 & paper 2.</p>
      </a>

    </div>

    

    <div class="home__sections">
      <div class="home__latest mt4">

        <h2 class="fs32 mb1">{{t "Latest Articles"}}</h2>

        <hr class="line mb1">

        {{#get "posts" limit="6" include="tags,tiers"}}
        <div class="home_latest">
          {{#foreach posts}}

          {{!-- <div class="article-link">
            <div>
              <div class="fs14 color-grey b ttu">
                {{#if primary_tag}}
                {{#primary_tag}}
                <a href="{{url}}" title="{{name}}" class="nolink">{{name}}</a>
                {{/primary_tag}}
                &ensp;
                {{/if}}
                {{> access-label icon_size=16}}
              </div>
              <a href="{{url}}" title="{{title}}" class="nolink color-text">{{title}}</a>
            </div>

          </div> --}}
          <div class="article-link" style="box-shadow: rgba(149, 157, 165, 0.2) 0px 8px 24px;"
            class="bg-white mb-4 p-3 box-episode box-post post-800 post type-post status-publish format-standard hentry category-writing tag-blog-post">
            <a href="{{url}}" title="{{title}}" class="nolink color-text">
              <div class="row">
                <div class="col-md-12">
                  <h3>{{title}}</h3>
                  {{!-- <h4 class="normal h6">August 10, 2022</h4> --}}
                  <div class="fs14 color-grey b ttu">
                    {{!-- {{#if primary_tag}}
                    {{#primary_tag}}
                    <a href="{{url}}" title="{{name}}" class="nolink">{{name}}</a>
                    {{/primary_tag}}
                    &ensp;
                    {{/if}} --}}
                    {{!-- {{#foreach tags limit="2"}}
                    <span class="mt1 mb1  fs14 b ttu color-grey">

                      <a class="tag_name" href='{{ url }}'> {{name}}</a>

                    </span>
                    {{/foreach}} --}}
                    {{> access-label icon_size=16}}
                  </div>
                  <p style="font-size: 15px;">{{{content words="10"}}}</p>
                </div>
              </div>
            </a>
          </div>
          {{/foreach}}

        </div>
        {{/get}}


      </div>
      <div class="home__latest mt4">

        <h2 class="fs32 mb1">{{t "Tags"}}</h2>

        <hr class="line mb1">

        <div style="display: flex; flex-wrap: wrap; gap: 10px 20px;">
          {{#get 'tags' limit='all' include='count.posts' order='count.posts desc'}}
          {{#foreach tags}}
          {{#if count.posts}}
          <a class="tag_name color-grey" href='{{ url }}'>
            <h4>{{ name }} <small>({{ count.posts }})</small></h4>
          </a>
          {{/if}}
          {{/foreach}}
          {{/get}}

        </div>

      </div>
      
      </div>
    </div>
  </div>



