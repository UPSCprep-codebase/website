{{!< default}} <div class="home-slant">
  </div>

  <div class="container">

    <div class="home__hero mt3 mb2">

      {{#page}}
      <div class="home__intro mt2">
        <div class="fs20">
          {{content}}
        </div>

        {{#if @custom.show_subscribe_form}}
        <div class="mt2 mb3">
          {{> subscribe-form-inline}}
        </div>
        {{/if}}

      </div>

      <div class="home__video">
        {{#if @custom.youtube_id}}
        <lite-youtube videoid="{{@custom.youtube_id}}"></lite-youtube>
        {{else}}
        {{#if feature_image}}
        <img src="{{img_url feature_image size=" m"}}" alt="{{title}}" />
        {{/if}}
        {{/if}}
      </div>
      {{/page}}

    </div>
    <div class="home__explore mt4">

        <h2 class="fs32 mb1">{{t "Testimonials"}}</h2>

        <hr class="line mb2">
        <div class="home_explore">

          {{> testimonial}}
        </div>
        <div class="kg-card kg-button-card kg-align-center"><a href="https://www.upscprep.com/testimonials/"
          class="kg-btn kg-btn-accent" target="_blank">More Testimonials</a></div>
    </div>
<div style="font-weight: 600;" class="home__checkout  fs24">
      {{@custom.callout}}
